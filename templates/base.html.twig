<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link
		rel="icon" href="{{ asset('build/images/ribbon.png') }}">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{% include 'include/_navbar.html.twig' %}
		<div class="row g-0">
			<div class="col-lg-2 bg-primary sidebar">
				{% include 'include/_sidebar.html.twig' %}
			</div>
			<div class="col-lg-10">
			{% include 'include/_navinfo.html.twig' %}
		{% block body %}{% endblock %}
		</div>
		{% include 'include/_footer.html.twig' %}
		</div>
	</body>
</html>
